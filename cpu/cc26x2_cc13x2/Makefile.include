CPU_VARIANT = x2

INCLUDES += -I$(RIOTCPU)/cc26x2_cc13x2/vendor

VECTORS_O = $(BINDIR)/cc26xx_cc13xx/vectors.o

ROM_START_ADDR ?= 0x00000000
RAM_START_ADDR ?= 0x20000000
ROM_LEN ?= 0x57FA8
RAM_LEN ?= 80K

LINKER_SCRIPT = $(RIOTCPU)/cc26x2_cc13x2/ldscripts/cc26x2_cc13x2.ld

include ${RIOTCPU}/cc26xx_cc13xx/Makefile.include
include $(RIOTMAKE)/arch/cortexm.inc.mk

INCLUDES += -I$(RIOTCPU)/cc26x2_cc13x2/include/vendor
